{
  "rewrites": [
    {
      "source": "/karsu-admin-panel",
      "destination": "/index.html"
    },
    {
      "source": "/karsu-admin-panel/(.*)",
      "destination": "/index.html"
    }
  ],
  "routes": [
    {
      "src": "/karsu-admin-panel/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/karsu-admin-panel/(.*)",
      "dest": "/index.html"
    }
  ]
}